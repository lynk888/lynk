{
  "expo": {
    "name": "lynk",
    "slug": "lynk",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ADD8E6"
    },
    "scheme": "lynk",
    "userInterfaceStyle": "automatic",
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.lynk"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#ADD8E6"
      },
      "package": "com.lynk"
    },
    "plugins": [
      [
        "expo-router",
        {
          "origin": "https://lynk.app"
        }
      ]
    ]
  }
}
